<fx:root prefHeight="70.0" prefWidth="1024.0" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.121"
         xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox alignment="CENTER_RIGHT" spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0"
              AnchorPane.topAnchor="10.0">
            <children>
                <Button fx:id="toRed" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="Setting"
